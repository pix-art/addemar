{
    "name": "pix-art/addemar",
    "type": "library",
    "description": "PHP Library to easy talk to the Addemar API",
    "keywords": ["Addemar", "Mailing", "Pix-art"],
    "homepage": "https://github.com/pix-art/addemar",
    "license": "MIT",
    "authors": [
        {
            "name": "Joeri Timmermans",
            "email": "joeri@pix-art.be"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "config": {
        "bin-dir": "bin",
        "cache-files-ttl": 0
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-0" : {
            "Addemar" : "src/",
            "Addemar\\Tests": "tests/"
        }
    }
}